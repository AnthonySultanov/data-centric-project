<body>
    <div>
      

        <br />
<!-- table with eid,name,role,salary,update and update is a button -->
        <table>
            <tr>
                <th>EID</th>
                <th>Name</th>
                <th>Role</th>
                <th>Salary</th>
                <th>Update</th>
            </tr>

            <% for(let e of employees) {%>
            <tr>
                <td>
                    <%= e.eid %>
                </td>
                <td>
                    <%= e.ename %>
                </td>
                <td>
                    <%= e.role %>
                </td>
                <td>
                    <%= e.salary %>
                </td>
                <td>
                    <a href="/employees/edit/<%= e.eid %>">Update</a>
                </td>
            </tr>
            <% }%>

        </table>
        <br>
        <a href="/">Home</a>
    </div>
</body>